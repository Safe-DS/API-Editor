{
    "name": "api-editor-client",
    "version": "0.0.0",
    "private": true,
    "homepage": "https://lars-reimann.github.io/api-editor",
    "scripts": {
        "dev": "vite",
        "build": "tsc && vite build",
        "serve": "vite preview",
        "test": "jest src",
        "eslint": "eslint .",
        "stylelint": "stylelint \"**/*.css\"",
        "lint": "pnpm run eslint && pnpm run stylelint",
        "fix": "eslint --fix . && stylelint --fix \"**/*.css\" && prettier --write .",
        "deploy": "pnpm run build && gh-pages -d dist",
        "prepare": "cd .. && husky install client/.husky"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": "eslint --fix .",
        "*.css": "stylelint --fix \"**/*.css\"",
        "*.{html,md,css,js,jsx,ts,tsx,json,yml}": "prettier --write ."
    },
    "dependencies": {
        "@emotion/react": "^11.4.0",
        "@emotion/styled": "^11.3.0",
