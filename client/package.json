    "devDependencies": {
        "@babel/core": "^7.0.0-0",
        "@types/jest": "^26.0.24",
        "@types/react": "^17.0.0",
        "@types/react-dom": "^17.0.0",
        "@types/react-router-dom": "^5.1.8",
        "@types/react-syntax-highlighter": "^13.5.2",
        "@types/uuid": "^8.3.1",
