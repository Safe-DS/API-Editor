        "eslint-plugin-import": "^2.23.4",
        "eslint-plugin-jest": "^24.3.7",
        "eslint-plugin-jsx-a11y": "^6.4.1",
        "eslint-plugin-react": "^7.24.0",
        "eslint-plugin-react-hooks": "^4.2.0",
        "eslint-plugin-testing-library": "^3.9.0",
        "gh-pages": "^3.2.3",
        "husky": "^7.0.1",
        "jest": "^27.0.6",
        "lint-staged": "^11.0.1",
