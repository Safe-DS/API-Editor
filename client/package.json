        "@material-ui/core": "^5.0.0-beta.1",
        "@material-ui/lab": "^5.0.0-alpha.40",
        "@popperjs/core": "^2.9.2",
        "@reduxjs/toolkit": "^1.6.1",
        "bootstrap": "^5.0.2",
        "classnames": "^2.3.1",
        "formik": "^2.2.9",
        "immutable": "^4.0.0-rc.14",
        "katex": "^0.13.11",
        "react": "^17.0.0",
