        "react-router": "^5.2.0",
        "react-router-dom": "^5.2.0",
        "react-syntax-highlighter": "^15.4.3",
        "rehype-katex": "^5.0.0",
        "remark-gfm": "^1.0.0",
        "remark-math": "^4.0.0",
        "uuid": "^8.3.2"
